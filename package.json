{
  "name": "trpc-cli",
  "version": "0.9.2",
  "description": "Turn a tRPC router into a type-safe, fully-functional, documented CLI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "dist/bin.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "cp-zod-to-json-schema": "node -e \"const pkg=require('./package.json'); const version=pkg.devDependencies['zod-to-json-schema']; const gitHead=require('child_process').execSync('npm view zod-to-json-schema@'+version+' gitHead',{encoding:'utf8'}).trim(); require('child_process').execSync('git clone https://github.com/StefanTerdell/zod-to-json-schema.git /tmp/zod-to-json-schema-temp && cd /tmp/zod-to-json-schema-temp && git checkout '+gitHead+' && cd - && cp -r /tmp/zod-to-json-schema-temp/src src/zod-to-json-schema && rm -rf /tmp/zod-to-json-schema-temp',{stdio:'inherit'})\"",
    "prepare": "pnpm build",
    "lint": "eslint --max-warnings=0 .",
    "clean": "rm -rf dist",
    "compile": "tsc -p tsconfig.lib.json",
    "build": "pnpm clean && pnpm compile",
    "dev": "cd test/fixtures && tsx",
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmkal/trpc-cli.git"
  },
  "keywords": [
    "tprc",
    "cli",
    "typescript"
  ],
  "author": "mmkal",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mmkal/trpc-cli/issues"
  },
  "homepage": "https://github.com/mmkal/trpc-cli#readme",
  "peerDependencies": {
    "@inquirer/prompts": "*",
    "omelette": "*"
  },
  "peerDependenciesMeta": {
    "omelette": {
      "optional": true
    },
    "@inquirer/prompts": {
      "optional": true
    }
  },
  "dependencies": {
    "commander": "^14.0.0"
  },
  "devDependencies": {
    "@clack/prompts": "0.11.0",
    "@inquirer/prompts": "7.5.1",
    "@orpc/contract": "1.5.0",
    "@orpc/server": "1.5.0",
    "@trpc/server": "11.4.3",
    "@types/json-schema": "7.0.15",
    "@types/node": "20.17.48",
    "@types/omelette": "0.4.5",
    "@types/prompts": "2.4.9",
    "@valibot/to-json-schema": "1.2.0",
    "arktype": "^2.1.20",
    "effect": "3.15.2",
    "enquirer": "2.4.1",
    "eslint": "8",
    "eslint-plugin-mmkal": "0.10.2",
    "execa": "9.3.1",
    "expect-type": "1.2.1",
    "fs-syncer": "0.5.3",
    "np": "10.2.0",
    "pkg-pr-new": "^0.0.50",
    "prompts": "2.4.2",
    "strip-ansi": "7.1.0",
    "trpcserver10": "npm:@trpc/server@^10.45.2",
    "trpcserver11": "npm:@trpc/server@^11.1.1",
    "tsx": "4.19.4",
    "typescript": "5.8.3",
    "valibot": "1.1.0",
    "vitest": "3.1.4",
    "zod": "3.25.76",
    "zod-to-json-schema": "3.24.6"
  }
}
